{
  "name": "playnite-web",
  "version": "3.1.4",
  "private": true,
  "engines": {
    "node": "22.10.0",
    "yarn": "4.5.1"
  },
  "workspaces": [
    "apps/*",
    "libs/**/*",
    "hass-*/",
    ".devcontainer"
  ],
  "devDependencies": {
    "@andrew-codes/playnite-plugin-installer-manifest": "workspace:*",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.0",
    "@semantic-release/exec": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^10.3.5",
    "@semantic-release/release-notes-generator": "^14.0.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.17.6",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "dotenv": "^16.4.5",
    "glob": "^10.4.5",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-config": "^29.7.0",
    "nx": "^19.8.11",
    "prettier": "^3.3.3",
    "semantic-release": "^24.2.0",
    "ts-jest": "^29.2.5",
    "tslib": "^2.8.1",
    "typedoc": "^0.26.11",
    "typedoc-plugin-markdown": "^4.2.10",
    "typescript": "^5.6.3",
    "typescript-remix-routes-plugin": "^1.0.1"
  },
  "dependenciesMeta": {
    "@nx/jest@17.1.3": {
      "unplugged": true
    },
    "prettier@3.3.2": {
      "unplugged": true
    }
  },
  "resolutions": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "cypress": "^13.15.0",
    "ws@^7.4.5": "^7.5.10",
    "ws@^7.5.5": "^7.5.10",
    "braces@~3.0.2": "^3.0.3",
    "braces@^3.0.2": "^3.0.3",
    "ip@^2.0.0": "2.0.1",
    "graphql": "16.9.0"
  },
  "packageManager": "yarn@4.5.1",
  "scripts": {
    "docs/generate": "typedoc --tsconfig tsconfig.typedoc.json",
    "prepare": "husky",
    "start": "nx run playnite-web-app:start",
    "test/components": "nx run playnite-web-app:test/components",
    "test/components/update": "nx run playnite-web-app:test/components/update",
    "test/components/ci": "nx run playnite-web-app:test/components/ci",
    "test/e2e": "nx run playnite-web-app:test/e2e",
    "test/e2e/update": "nx run playnite-web-app:test/e2e/update",
    "test/e2e/ci": "nx run playnite-web-app:test/e2e/ci",
    "test/unit": "nx run-many --target=test/unit",
    "test/unit/ci": "nx run-many --target=test/unit/ci"
  }
}
