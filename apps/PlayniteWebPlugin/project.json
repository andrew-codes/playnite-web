{
  "name": "playnite-web-plugin",
  "sourceRoot": "apps/playnite-web-extension/src",
  "projectType": "application",
  "targets": {
    "clean": {
      "executor": "nx:run-commands",
      "options": {
        "command": "yarn rimraf .dist .packaged .test-runs",
        "cwd": "{projectRoot}"
      }
    },
    "build": {
      "executor": "nx:run-commands",
      "outputs": ["{projectRoot}/.dist"],
      "options": {
        "command": "msbuild PlayniteWeb.csproj -verbosity:diag -property:Configuration=Release",
        "cwd": "{projectRoot}/src"
      }
    },
    "build/restore": {
      "executor": "nx:run-commands",
      "options": {
        "command": "nuget restore",
        "cwd": "{projectRoot}/src"
      }
    },
    "package": {
      "executor": "nx:run-commands",
      "options": {
        "commands": "./scripts/package.sh",
        "cwd": "{projectRoot}"
      }
    }
  },
  "tags": ["windows"],
  "implicitDependencies": []
}
