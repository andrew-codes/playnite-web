{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "jsx": "react",
    "esModuleInterop": true,
    "noEmit": false,
    "allowJs": true,
    "module": "ESNext",
    "moduleResolution": "Node",
    "rootDir": "src",
    "types": [
      "@types/cypress",
      "@types/node",
      "@types/react"
    ],
  },
  "include": [
    "src/**/*.ts",
    "src/**/*.tsx",
    "src/**/*.js",
    "src/**/*.jsx"
  ],
}
