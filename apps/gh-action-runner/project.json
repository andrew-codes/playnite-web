{
  "name": "gh-action-runner",
  "sourceRoot": "apps/gh-action-runner/src",
  "projectType": "application",
  "targets": {
    "clean": {
      "executor": "nx:run-commands",
      "options": {
        "command": "yarn rimraf .dist _packaged .test-runs build public/build",
        "cwd": "{projectRoot}"
      }
    },
    "package": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "yarn node --register esbuild-register scripts/package.js"
        ],
        "cwd": "{projectRoot}"
      }
    },
    "publish": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "yarn node --register esbuild-register scripts/publish.js"
        ],
        "cwd": "{projectRoot}"
      }
    }
  },
  "tags": ["linux"],
  "implicitDependencies": []
}
