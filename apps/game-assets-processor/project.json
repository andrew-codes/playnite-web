{
  "name": "game-assets-processor",
  "sourceRoot": "apps/game-assets-processor/src",
  "projectType": "application",
  "targets": {
    "clean": {
      "executor": "nx:run-commands",
      "dependsOn": ["clean/build", "clean/package", "clean/test"],
      "options": {
        "parallel": true,
        "commands": []
      }
    },
    "clean/build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "yarn rimraf build",
        "cwd": "{projectRoot}"
      }
    },
    "clean/package": {
      "executor": "nx:run-commands",
      "options": {
        "command": "yarn rimraf _packaged",
        "cwd": "{projectRoot}"
      }
    },
    "clean/test": {
      "executor": "nx:run-commands",
      "options": {
        "command": "yarn rimraf .test-runs .nyc_output",
        "cwd": "{projectRoot}"
      }
    },
    "prepare": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "{projectRoot}",
        "command": "echo 'No prepare step...'"
      }
    },
    "start": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "{projectRoot}",
        "parallel": false,
        "commands": ["yarn tsx scripts/start.ts"],
        "env": {
          "NODE_ENV": "development",
          "LOG_LEVEL": "silly",
          "DATABASE_URL": "postgresql://local:dev@localhost:5432/games?schema=public",
          "ASSET_PATH": "./.game-assets"
        }
      }
    },
    "test/e2e": {
      "executor": "nx:run-commands",
      "options": {
        "parallel": false,
        "commands": ["yarn tsx scripts/e2e.ts"],
        "cwd": "{projectRoot}",
        "env": {
          "DATABASE_URL": "postgresql://local:dev@localhost:5432/games?schema=public",
          "ASSET_PATH": "./.game-assets",
          "INSTRUMENT": "true",
          "LOCAL": "true",
          "CI": "true",
          "TEST": "E2E",
          "LOG_LEVEL": "debug",
          "NODE_ENV": "production",
          "PORT": "3001"
        }
      }
    },
    "build": {
      "executor": "nx:run-commands",
      "defaultConfiguration": "production",
      "options": {
        "cwd": "{projectRoot}",
        "commands": ["yarn tsx scripts/build.ts"],
        "env": {
          "NODE_ENV": "production"
        }
      }
    },
    "build/e2e": {
      "executor": "nx:run-commands",
      "defaultConfiguration": "production",
      "options": {
        "cwd": "{projectRoot}",
        "commands": ["yarn tsx scripts/build.ts"],
        "env": {
          "NODE_ENV": "production",
          "INSTRUMENT": "true"
        }
      }
    },
    "package": {
      "executor": "nx:run-commands",
      "options": {
        "parallel": false,
        "commands": [],
        "cwd": "{projectRoot}"
      },
      "defaultConfiguration": "production",
      "configurations": {
        "production": {
          "commands": [
            "yarn tsx scripts/packageFiles.ts",
            "yarn tsx scripts/package.ts"
          ]
        }
      }
    },
    "package/e2e": {
      "executor": "nx:run-commands",
      "options": {
        "parallel": false,
        "commands": ["yarn tsx scripts/packageFiles.ts"],
        "cwd": "{projectRoot}"
      }
    },
    "package/prune": {
      "executor": "nx:run-commands",
      "options": {
        "parallel": false,
        "commands": ["yarn tsx scripts/packagePrune.ts"],
        "cwd": "{projectRoot}"
      }
    },
    "publish": {
      "executor": "nx:run-commands",
      "options": {
        "parallel": false,
        "commands": ["yarn tsx scripts/publish.ts"],
        "cwd": "{projectRoot}"
      }
    }
  },
  "tags": ["linux"],
  "implicitDependencies": []
}
