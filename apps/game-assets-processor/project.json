{
  "name": "game-assets-processor",
  "sourceRoot": "apps/game-assets-processor/src",
  "projectType": "application",
  "targets": {
    "clean": {
      "executor": "nx:run-commands",
      "dependsOn": ["clean/build", "clean/package", "clean/test"],
      "options": {
        "parallel": true,
        "commands": []
      }
    },
    "clean/build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "yarn rimraf build",
        "cwd": "{projectRoot}"
      }
    },
    "clean/package": {
      "executor": "nx:run-commands",
      "options": {
        "command": "yarn rimraf _packaged",
        "cwd": "{projectRoot}"
      }
    },
    "clean/test": {
      "executor": "nx:run-commands",
      "options": {
        "command": "yarn rimraf .test-runs .nyc_output",
        "cwd": "{projectRoot}"
      }
    },
    "prepare": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "{projectRoot}",
        "command": "echo 'No prepare step...'"
      }
    },
    "start": {
      "executor": "nx:run-commands",
      "inputs": [],
      "options": {
        "cwd": "{projectRoot}",
        "parallel": false,
        "commands": ["yarn tsx scripts/start.ts"],
        "env": {
          "NODE_ENV": "development",
          "LOG_LEVEL": "silly",
          "DATABASE_URL": "postgresql://local:dev@localhost:5432/games?schema=public",
          "ASSET_PATH": "../.game-assets"
        }
      }
    },
    "test/unit": {
      "executor": "nx:run-commands",
      "dependsOn": ["clean/test"],
      "inputs": ["src", "{projectRoot}/jest.config.js"],
      "outputs": ["{projectRoot}/.test-runs/unit"],
      "options": {
        "command": "yarn jest --config jest.config.js",
        "cwd": "{projectRoot}"
      },
      "configurations": {
        "ci": {}
      }
    },
    "test/e2e": {
      "executor": "nx:run-commands",
      "inputs": ["src", "{projectRoot}/jest.config.e2e.js"],
      "outputs": ["{projectRoot}/.test-runs/e2e"],
      "options": {
        "parallel": false,
        "commands": [
          "yarn nx run game-assets-processor:build:e2e",
          "yarn tsx scripts/packageFiles.ts",
          "yarn tsx scripts/package.ts",
          "yarn tsx scripts/e2e.ts"
        ],
        "cwd": "{projectRoot}"
      },
      "defaultConfiguration": "ci",
      "configurations": {
        "ci": {
          "env": {
            "DATABASE_URL": "postgresql://local:dev@localhost:5432/games?schema=public",
            "ASSET_PATH": "./game-assets",
            "INSTRUMENT": "true",
            "LOCAL": "true",
            "CI": "true",
            "LOG_LEVEL": "error",
            "NODE_ENV": "production"
          }
        }
      }
    },
    "build": {
      "executor": "nx:run-commands",
      "defaultConfiguration": "production",
      "dependsOn": ["clean/build"],
      "options": {
        "cwd": "{projectRoot}",
        "commands": ["yarn tsx scripts/build.ts"]
      },
      "configurations": {
        "production": {
          "env": {
            "NODE_ENV": "production"
          }
        },
        "e2e": {
          "env": {
            "NODE_ENV": "production",
            "INSTRUMENT": "true"
          }
        }
      }
    },
    "package": {
      "inputs": [
        "{projectRoot}/package.json",
        "{projectRoot}/build/**",
        "{projectRoot}/scripts/packageFiles.ts",
        "{projectRoot}/scripts/package.ts"
      ],
      "outputs": ["{projectRoot}/_packaged"],
      "executor": "nx:run-commands",
      "dependsOn": ["clean/package", "build"],
      "options": {
        "parallel": false,
        "commands": [],
        "cwd": "{projectRoot}"
      },
      "defaultConfiguration": "production",
      "configurations": {
        "production": {
          "commands": [
            "yarn tsx scripts/packageFiles.ts",
            "yarn tsx scripts/package.ts"
          ]
        }
      }
    },
    "package/prune": {
      "executor": "nx:run-commands",
      "options": {
        "parallel": false,
        "commands": ["yarn tsx scripts/packagePrune.ts"],
        "cwd": "{projectRoot}"
      }
    },
    "publish": {}
  },
  "tags": ["linux"],
  "implicitDependencies": []
}
