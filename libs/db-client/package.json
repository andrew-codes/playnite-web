{
  "name": "db-client",
  "version": "0.0.1",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "main": "build/client.js",
  "types": "build/client.ts",
  "files": [
    "src",
    "build",
    ".generated"
  ],
  "exports": {
    ".": {
      "import": "./build/client.js",
      "types": "./src/client.ts",
      "require": "./build/client.cjs",
      "default": "./build/client.js"
    },
    "./migrate": {
      "import": "./src/migrate.ts",
      "types": "./src/migrate.ts",
      "default": "./src/migrate.ts"
    }
  },
  "dependencies": {
    "@prisma/client": "6.14.0",
    "dev-logger": "workspace:*",
    "prisma": "6.14.0"
  },
  "devDependencies": {
    "@types/node": "^22.16.5",
    "@yarnpkg/pnpify": "^4.1.5",
    "esbuild": "^0.25.9",
    "glob": "^11.0.3",
    "jest": "^30.0.5",
    "nx": "^21.4.1",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "shelljs": "^0.10.0",
    "ts-jest": "^29.4.1",
    "tslib": "^2.8.1",
    "tsx": "^4.20.4",
    "typescript": "^5.9.2"
  }
}
