{
  "name": "sourced-assets",
  "version": "0.0.1",
  "private": true,
  "sideEffects": false,
  "main": "./src/index.ts",
  "files": [
    "src",
    "build"
  ],
  "exports": {
    ".": {
      "import": "./src/index.ts",
      "default": "./src/index.ts"
    },
    "./ign": {
      "import": "./src/ign.ts",
      "default": "./src/ign.ts"
    },
    "./slug": {
      "import": "./src/slug.ts",
      "default": "./src/slug.ts"
    },
    "./errors": {
      "import": "./src/errors.ts",
      "default": "./src/errors.ts"
    }
  },
  "dependencies": {
    "dev-logger": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^24.10.4",
    "glob": "^13.0.0",
    "jest": "^30.2.0",
    "nx": "^22.3.3",
    "prettier": "^3.7.4",
    "rimraf": "^6.1.2",
    "shelljs": "^0.10.0",
    "ts-jest": "^29.4.6",
    "tslib": "^2.8.1",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  }
}
