    "jest": "^30.0.5",
    "nx": "^21.4.1",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "shelljs": "^0.10.0",
    "ts-jest": "^29.4.1",
    "tslib": "^2.8.1",
    "tsx": "^4.20.4",
    "typescript": "^5.9.2"
  }
