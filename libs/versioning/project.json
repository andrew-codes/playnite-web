{
  "name": "versioning",
  "sourceRoot": "libs/versioning/src",
  "projectType": "library",
  "targets": {
    "clean": {
      "executor": "nx:run-commands",
      "options": {
        "command": "yarn rimraf .test-runs",
        "cwd": "{projectRoot}"
      }
    },
    "prepare": {
      "executor": "nx:run-commands",
      "outputs": ["{projectRoot}/.generated"],
      "options": {
        "parallel": false,
        "commands": ["echo 'Preparing versioning...'"],
        "cwd": "{projectRoot}"
      }
    }
  },
  "tags": ["linux"],
  "implicitDependencies": []
}
